<!-- START JUMBOTRON -->
<div class="jumbotron" data-pages="parallax">
    <div class=" container-fluid   container-fixed-lg sm-p-l-0 sm-p-r-0">
        <div class="inner">
            <!-- START BREADCRUMB -->
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="/dashboard">Home</a>
                </li>
                <li class="breadcrumb-item active">Corporate List</li>
            </ol>
            <!-- END BREADCRUMB -->
        </div>
    </div>
</div>
<!-- END JUMBOTRON -->


<!-- START card -->
<div class="card card-transparent">
    <div class="card-header ">
        {{#if file_assignment}}
        <div class="alert alert-success">
            <strong>Success! </strong>{{file_assignment}}
        </div>
        {{/if}}
        {{#if success_add_lawyers}}
        <div class="alert alert-success">
            <strong>Success! </strong>{{success_add_lawyers}}
        </div>
        {{/if}} {{#if delete_add_lawyers}}
        <div class="alert alert-success">
            <strong>Success! </strong>{{delete_add_lawyers}}
        </div>
        {{/if}} {{#if edit_lawyers}}
        <div class="alert alert-success">
            <strong>Success! </strong>{{edit_lawyers}}
        </div>
        {{/if}}
        <div class="card-title">
            <h5>All Cases</h5>
        </div>
        <div class="pull-right">
            <a href="javascript:void(0)" class="btn btn-primary btn-cons m-b-10" id="lawyerAssignment">
                <i class="fa fa-share-square-o"></i>Assignment</a>
            <div class="col-xs-12">
                {{!-- <input type="text" id="search-table" class="form-control pull-right" placeholder="Search"> --}}
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="card-body lawyers-add">
        <table class="table table-hover demo-table-search table-responsive-block data-tables" id="tableWithSearch">
            <thead>
                <tr>
                    <th>Sl No.</th>
                    <th>Corporate</th>
                    <th>No of Case</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                {{#each customerDetails}}
                <tr class="">
                    <td class="v-align-middle semi-bold">
                        <p>{{math @index "+" 1}}</p>
                    </td>
                    <td class="v-align-middle">
                        <img src="{{this.customers.0.logo}}" width="40" height="40">
                    </td>
                    <td class="v-align-middle">
                        <p>{{this.all_cases.0.length}}</p>
                    </td>
                    <td>
                        <a href="/case-conversation/{{this.id}}" class="btn btn-complete"> <i class="fa fa-eye"></i>
                        </a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>




{{#content "script" mode="append"}}
<script src="/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js" type="text/javascript"></script>
<script type="text/javascript" src="/assets/plugins/jquery-inputmask/jquery.inputmask.min.js"></script>

<script type="text/javascript">



</script> {{/content}}
<!-- END card -->