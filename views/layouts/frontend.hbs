<body>


    <!-- Starts on register-box -->

    <div class="register">
        <a href="" data-toggle="modal" data-target="#register-model">
            <i class="fa fa-book"></i> Request a Call back</a>
    </div>

    <!-- Modal-->
    <div class="modal fade lgx-modal" id="register-model">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button aria-hidden="true" data-dismiss="modal" class="close" type="button">×</button>
                    <h4 class="modal-title">Request a Call back</h4>
                </div>
                <div class="modal-body">
                    {{!-- <h6>Call us 24/7 at 858.122.2450 or fill out the form be;;ow to receive a free and confidential initial consulation.</h6> --}}
                    <form id="bookCallBackFrm" name="bookCallBackFrm">
                        <div class="form-group">
                            <label for="name">Name</label>
                            <input type="text" class="form-control" id="call_back_name" name="call_back_name"
                                aria-describedby="emailHelp" placeholder="Enter Name">
                        </div>
                        <div class="form-group">
                            <label for="mobile">Mobile</label>
                            <input type="text" class="form-control" id="call_back_mobile" name="call_back_mobile"
                                aria-describedby="emailHelp" placeholder="Enter Mobile" maxlength="10">
                        </div>
                        <div class="form-group">
                            <label for="email">Email</label>
                            <input type="email" class="form-control" id="call_back_email" name="call_back_email"
                                aria-describedby="emailHelp" placeholder="Enter email">
                        </div>
                        <div class="form-group">
                            <label for="messages">Messages</label>
                            <textarea class="form-control" id="call_back_messages" name="call_back_messages"
                                placeholder="Enter Messages"></textarea>
                        </div>
                        <button type="submit" class="btn btn-primary">Book</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- //.Modal-->

    <!-- Ends on register-box -->
    {{> f_header}}

    <!-- Ends Header -->

    {{{body}}}

    <!-- Starts Footer Section -->

