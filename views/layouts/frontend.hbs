    </script>

    <script>
        {{!-- $(document).ready(function () {
            $('select').selectpicker();
        }); --}}

        $(document).on("change", "#state",function() {
            var state_id = $("#state").val(); 
            $.ajax({
                type: "POST",
                url: "/get-city",
                data: {
                    state_id: state_id
                },
                success: function (response) {
                    if (response.success == true) {
                        $("#city").find('option').not(':first').remove();
                        for (var i = 0; i < response.city.length; i++) {
                            $("#city").append('<option value="' + response.city[i].id + '">' + response.city[i].name + '</option>');
                        }
                    }
                },
                error: function (xhr) {

                }
            });
        });
        $(document).ready(function(){
            $("#freeConsultFrm").validate({
                rules:
                {
                    state: {
                        required: true
                    },
                    city: {
                        required: true
                    },
                    name: {
                        required: true
                    },
                    email: {
                        required: true,
                        email:true
                    },
                    mobile: {
                        required: true,
                        number: true
                    },
                    subject: {
                        required: true
                    },
                    messages: {
                        required: true
                    }
                },
                messages: 
                {
                    state: {
                        required: "Please select state"
                    },
                    city: {
                        required: "Please select city"
                    },
                    name: {
                        required: "Name can't be blank"
                    },
                    email: {
                        required: "Email Id can't be blank"
                    },
                    email: {
                        required: "Mobile no can't be blank"
                    },
                    subject: {
                        required: "Subject can't be blank"
                    },
                    messages: {
                        required: "Messages can't be blank"
                    }
                },
                submitHandler: function (form) {
                    $.ajax({
                        type: "POST",
                        url: "/add/free-legal-consultation",
                        data: $("#freeConsultFrm").serialize(),
                        success:function(response)
                        {
                            if(response.success == true)
                            {
                                swal({
                                    title: "Thank you!",
                                    text: "For sending this Quote",
                                    type: "success"
                                }, function () {
                                    window.location.href = "/";
                                });
                            }
                        },
                        error: function(xhr)
                        {

                        }
                    })
                }
            });

            $("#bookCallBackFrm").validate({
                rules:
                {
                    call_back_name: {
                        required: true
                    },
                    call_back_email: {
                        required: true,
                        email:true
                    },
                    call_back_mobile: {
                        required: true,
                        number: true
                    },
                    call_back_messages: {
                        required: true
                    }
                },
                messages: 
                {
                    call_back_name: {
                        required: "Name can't be blank"
                    },
                    call_back_email: {
                        required: "Email Id can't be blank"
                    },
                    call_back_mobile: {
                        required: "Mobile no can't be blank"
                    },
                    call_back_messages: {
                        required: "Messages can't be blank"
                    }
                },
                submitHandler: function (form) {
                    $.ajax({
                        type: "POST",
                        url: "/book-a-call-back",
                        data: $("#bookCallBackFrm").serialize(),
                        success:function(response)
                        {
                            if(response.success == true)
                            {
                                swal({
                                    title: "Thank you!",
                                    text: "We will contact soon",
                                    type: "success"
                                }, function () {
                                    window.location.reload();
                                });
                            }
                        },
                        error: function(xhr)
                        {

                        }
                    })
                }
            });
        });

        $(document).on("click", "#bookCallBtn", function(){
            $(".bookCallBackModal").modal("show");
        })
    </script>

    <!-- STARTS SCROLL TO TOP -->
    <script>
      $(document).ready(function () {

        $(window).scroll(function () {
          if ($(this).scrollTop() > 100) {
            $('.scrollup').fadeIn();
          } else {
            $('.scrollup').fadeOut();
          }
        });

        $('.scrollup').click(function () {
          $("html, body").animate({
            scrollTop: 0
          }, 600);
          return false;
        });

      });
    </script>


