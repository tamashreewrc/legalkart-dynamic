$(document).on("click","#addBroadcastSentBtn", function(){
    if($("#broadcast_msg").val() == "")
    {
        swal("Warning!","Please enter message","warning");
    }
    else
    {
        $.ajax({
            type: "POST",
            url: "/send-broadcast-msg",
            data: {
                broadcast_msg: $("#broadcast_msg").val()
            },
            beforeSend: function() {
                    // setting a timeout
                    $("#addBroadcastSentBtn").attr("disabled",true);
                    $("#addBroadcastSentBtn").html('Please Wait...');
                },
            success: function(response)
            {
                if(response.success == true)
                {
                    swal({
                        title: "Success!",
                        text: "Message has been sent",
                        type: "success"
                    }, function () {
                        window.location.reload();
                    });
                }
            },
            error: function(xhr)
            {

            }
        })
    }
})
