$(document).on("click","#changeLoginRoleByCorporateAdmin", function(){
    $.ajax({
        type: "GET",
        url: "/Get-city-manager-list",
        success: function(response)
        {
            if(response.success == true)
            {
                $("#city_manager_id").find('option').not(':first').remove();
                for(var i=0; i<response.city_manager_list.length; i++)
                {
                    if({{user.role_id}} == "5")
                    {
                        if(response.city_manager_list[i].super_admin_status != "1")
                        $("#city_manager_id").append(`
                            <option value="${response.city_manager_list[i].email},${response.city_manager_list[i].password}">${response.city_manager_list[i].first_name} ${response.city_manager_list[i].last_name}</option>
                        `);
                    }
                    else
                    {
                        $("#city_manager_id").append(`
                            <option value="${response.city_manager_list[i].email},${response.city_manager_list[i].password}">${response.city_manager_list[i].first_name} ${response.city_manager_list[i].last_name}</option>
                        `);
                    }
                }
                $("#changeLoginRoleByCorporateAdminModal").modal('show');
            }
        },
        error: function(xhr)
        {

        }
    });
})

