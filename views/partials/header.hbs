                $("#changeLoginRoleByCorporateAdminfrm").validate({
                    rules: {
                        city_manager_id: {
                            required: true
                        }
                    },
                    messages: {
                        city_manager_id: {
                            required: "Please Select One"
                        }
                    }
                })
                $.ajax({
                    type: "GET",
                    url:"/total-message-count",
                    success: function(response){
                        if(response.success == true)
                        {
                            if (response.new_msg>0) {
                                $("#bubble_msg_count").addClass("bubble");
                            }
                        }
                    },
                    error: function(xhr)
                    {

                    }
                })
            });
            $(document).ready(function () {

                $.ajax({
                    type: "GET",
                    url: "/notification-legalcart",
                    success: function (response) {
                        console.log(response);
                        if (response.data.length>0) {
                            $("#bubble_").addClass("bubble");
                        }
                        for (let i = 0; i < response.data.length; i++) {

                            var todayTime = new Date(response.data[i].createdAt);
                            month = '' + (todayTime.getMonth() + 1),
                            day = '' + todayTime.getDate(),
                            year = todayTime.getFullYear();

                            if (month.length < 2) month = '0' + month;
                            if (day.length < 2) day = '0' + day;

                            var creation_date = [month, day, year].join('-');

                            var d = new Date(response.data[i].createdAt),
                            dformat = [d.getMonth() + 1,
                            d.getDate(),
                            d.getFullYear()].join('-') + ' ' +
                            [d.getHours(),
                            d.getMinutes(),
                            d.getSeconds()].join(':');

                            var header = "n";

                            var startDate = new Date(response.data[i].createdAt);

                            var endDate = new Date();
                            var diffDays = Math.ceil((endDate.getTime() - startDate.getTime()) / (1000 * 3600 * 24));
                            var diff = (endDate.getTime() - startDate.getTime()) / 1000;
                            var hour = diff / (60 * 60);
                            var minute = diff / 60;
                            var hrs = Math.abs(Math.round(hour));
                            var mins = Math.abs(Math.round(minute));
                            var sec = Math.abs(Math.round(diff));
                            var get_time = "";
                            if (sec < 60) {
                                get_time = "Just now";
                            }
                            else if (sec >= 60 && mins < 60 && diffDays == 1) {
                                get_time = mins + " mins ago";
                            }
                            else if (sec > 60 && mins >= 60 && diffDays == 1) {
                                get_time = hrs + " hours ago";
                            }
                            else if (sec > 60 && mins > 60 && diffDays > 1 && diffDays < 7) {
                                get_time = diffDays - 1 + " days ago"
                            }
                            else if (sec > 60 && mins > 60 && diffDays >= 7) {
                                get_time = dformat;
                            }
                            if({{user.role_id}} == "3")
                            {
                                $('#nitificationsList').append(`<div class="row" ><div class="m-t-10 col-md-2"><img class="border-rad-50p" width="30" height="30" data-src-retina="/frontend/images/favicon.ico" data-src="/frontend/images/favicon.ico" alt="" src="/frontend/images/favicon.ico"></div><div class="col-md-10"><a href="#" data-noti-id="` + response.data[i].id + `" data-noti-link="` + response.data[i].link + `"id="notification_change" class="text-primary line-h-1 m-t-10 d-block notification_change"><span class="bold m-b-10 d-block">LekalKart</span><span class="fs-12 d-block">` + response.data[i].remarks + `</span></a><span class="time">` + get_time + `</span></div></div>`)
                            }
                            else
                            {
                                $('#nitificationsList').append(`<div class="row" ><div class="m-t-10 col-md-2"><img class="border-rad-50p" width="30" height="30" data-src-retina="` + response.data[i].img + `" data-src="` + response.data[i].img + `" alt="" src="` + response.data[i].img + `"></div><div class="col-md-10"><a href="#" data-noti-id="`+ response.data[i].id +`" data-noti-link="`+ response.data[i].link +`"id="notification_change" class="text-primary line-h-1 m-t-10 d-block notification_change"><span class="bold m-b-10 d-block">` + response.data[i].name + `</span><span class="fs-12 d-block">` + response.data[i].remarks + `</span></a><span class="time">` + get_time + `</span></div></div>`)
                            }
                        }
                    },
                    error: function (xhr) {

                    },
                    complete: function () {
                        $('.notification_change').click(function () {
                            var noti_id = $(this).data("noti-id");
                            var noti_link = $(this).data("noti-link");
                            $.ajax({
                                type: "POST",
                                url: "/notification-update",
                                data: { noti_id: noti_id },
                                success: function (response) {
                                    window.location.href = noti_link
                                },
                                error: function (xhr) {

                                }
                            });
                        });
                    }
                });

